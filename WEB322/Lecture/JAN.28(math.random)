//Math.random() returns a random number between 0 (inclusive),  and 1 (exclusive):

function randomNumber(maxWaitting){
    return Math.floor(Math.random()*maxWaitting);
    
    }
    
    function a(){
        setTimeout(function(){
            console.log("a");
        },randomNumber(7000)); //7seconds
    }
    
    function b() {
        setTimeout(function(){
            console.log("b");
        },randomNumber(2000)); //2seconds
    
    }
    
    function c() {
        setTimeout(function(){
            console.log("c");
        },randomNumber(9000)); //9seconds
    }
    
    c();
    a();
    b();
